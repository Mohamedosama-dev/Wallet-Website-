{"ast":null,"code":"export default {\n  data() {\n    return {\n      recipientPhone: '',\n      amount: 0\n    };\n  },\n  methods: {\n    transfer() {\n      // Call the API to transfer money\n      fetch('/api/transfer/', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json',\n          'X-CSRFToken': this.getCSRFToken()\n        },\n        body: JSON.stringify({\n          target_phone: this.recipientPhone,\n          amount: this.amount\n        })\n      }).then(response => {\n        if (!response.ok) {\n          throw new Error('Network response was not ok');\n        }\n        return response.json();\n      }).then(data => {\n        // Handle success (e.g., show a success message, redirect, etc.)\n        alert('Transfer successful!');\n        this.recipientPhone = ''; // Reset fields after successful transfer\n        this.amount = 0;\n      }).catch(error => {\n        // Handle errors (e.g., show an error message)\n        console.error('Error during transfer:', error);\n        alert('Transfer failed. Please try again.');\n      });\n    },\n    getCSRFToken() {\n      // Get CSRF token from cookies\n      const name = 'csrftoken=';\n      const decodedCookie = decodeURIComponent(document.cookie);\n      const cookies = decodedCookie.split(';');\n      for (let cookie of cookies) {\n        while (cookie.charAt(0) === ' ') {\n          cookie = cookie.substring(1);\n        }\n        if (cookie.indexOf(name) === 0) {\n          return cookie.substring(name.length, cookie.length);\n        }\n      }\n      return '';\n    }\n  }\n};","map":{"version":3,"names":["data","recipientPhone","amount","methods","transfer","fetch","method","headers","getCSRFToken","body","JSON","stringify","target_phone","then","response","ok","Error","json","alert","catch","error","console","name","decodedCookie","decodeURIComponent","document","cookie","cookies","split","charAt","substring","indexOf","length"],"sources":["C:\\Users\\ADMIN\\Desktop\\q - copy-gold\\myproject\\wallet-app\\src\\components\\TransferComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"form-container\">\r\n      <h1>Transfer Money</h1>\r\n      <form @submit.prevent=\"transfer\">\r\n        <label>Recipient Phone:</label>\r\n        <input type=\"text\" v-model=\"recipientPhone\" required />\r\n        <label>Amount:</label>\r\n        <input type=\"number\" v-model=\"amount\" required />\r\n        <button type=\"submit\">Transfer</button>\r\n      </form>\r\n      <router-link to=\"/dashboard\" class=\"back-link\">Back to Dashboard</router-link>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      recipientPhone: '',\r\n      amount: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    transfer() {\r\n      // Call the API to transfer money\r\n      fetch('/api/transfer/', {\r\n        method: 'POST',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n          'X-CSRFToken': this.getCSRFToken(),\r\n        },\r\n        body: JSON.stringify({ target_phone: this.recipientPhone, amount: this.amount }),\r\n      })\r\n        .then(response => {\r\n          if (!response.ok) {\r\n            throw new Error('Network response was not ok');\r\n          }\r\n          return response.json();\r\n        })\r\n        .then(data => {\r\n          // Handle success (e.g., show a success message, redirect, etc.)\r\n          alert('Transfer successful!');\r\n          this.recipientPhone = ''; // Reset fields after successful transfer\r\n          this.amount = 0;\r\n        })\r\n        .catch(error => {\r\n          // Handle errors (e.g., show an error message)\r\n          console.error('Error during transfer:', error);\r\n          alert('Transfer failed. Please try again.');\r\n        });\r\n    },\r\n    getCSRFToken() {\r\n      // Get CSRF token from cookies\r\n      const name = 'csrftoken=';\r\n      const decodedCookie = decodeURIComponent(document.cookie);\r\n      const cookies = decodedCookie.split(';');\r\n      for (let cookie of cookies) {\r\n        while (cookie.charAt(0) === ' ') {\r\n          cookie = cookie.substring(1);\r\n        }\r\n        if (cookie.indexOf(name) === 0) {\r\n          return cookie.substring(name.length, cookie.length);\r\n        }\r\n      }\r\n      return '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Basic Reset */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Body Styles */\r\n.container {\r\n  font-family: Arial, sans-serif;\r\n  background-color: #f9f9f9;\r\n  color: #333;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  padding: 20px;\r\n}\r\n\r\n/* Form Container */\r\n.form-container {\r\n  background-color: #ffffff;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  padding: 20px;\r\n  max-width: 500px;\r\n  width: 100%;\r\n}\r\n\r\n/* Header Styles */\r\nh1 {\r\n  font-size: 2em;\r\n  color: #000000; /* Changed to black */\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n/* Form Styles */\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\nlabel {\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ninput {\r\n  padding: 10px;\r\n  margin-bottom: 15px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  background-color: #000000; /* Changed to black */\r\n  color: #ffffff;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 1em;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #808080; /* Changed to gray on hover */\r\n}\r\n\r\n/* Back Link */\r\n.back-link {\r\n  display: block;\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  color: #000000; /* Changed to black */\r\n  text-decoration: none;\r\n}\r\n\r\n.back-link:hover {\r\n  color: #808080; /* Changed to gray on hover */\r\n  text-decoration: underline;\r\n}\r\n\r\n/* Responsive Design */\r\n@media (max-width: 600px) {\r\n  .form-container {\r\n    width: 100%;\r\n    padding: 15px;\r\n  }\r\n\r\n  h1 {\r\n    font-size: 1.5em;\r\n  }\r\n\r\n  input {\r\n    padding: 8px;\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  button {\r\n    padding: 8px;\r\n    font-size: 0.9em;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAiBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT;MACAC,KAAK,CAAC,gBAAgB,EAAE;QACtBC,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACP,cAAc,EAAE,kBAAkB;UAClC,aAAa,EAAE,IAAI,CAACC,YAAY,CAAC;QACnC,CAAC;QACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;UAAEC,YAAY,EAAE,IAAI,CAACX,cAAc;UAAEC,MAAM,EAAE,IAAI,CAACA;QAAO,CAAC;MACjF,CAAC,EACEW,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChD;QACA,OAAOF,QAAQ,CAACG,IAAI,CAAC,CAAC;MACxB,CAAC,EACAJ,IAAI,CAACb,IAAG,IAAK;QACZ;QACAkB,KAAK,CAAC,sBAAsB,CAAC;QAC7B,IAAI,CAACjB,cAAa,GAAI,EAAE,EAAE;QAC1B,IAAI,CAACC,MAAK,GAAI,CAAC;MACjB,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;QACd;QACAC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;QAC9CF,KAAK,CAAC,oCAAoC,CAAC;MAC7C,CAAC,CAAC;IACN,CAAC;IACDV,YAAYA,CAAA,EAAG;MACb;MACA,MAAMc,IAAG,GAAI,YAAY;MACzB,MAAMC,aAAY,GAAIC,kBAAkB,CAACC,QAAQ,CAACC,MAAM,CAAC;MACzD,MAAMC,OAAM,GAAIJ,aAAa,CAACK,KAAK,CAAC,GAAG,CAAC;MACxC,KAAK,IAAIF,MAAK,IAAKC,OAAO,EAAE;QAC1B,OAAOD,MAAM,CAACG,MAAM,CAAC,CAAC,MAAM,GAAG,EAAE;UAC/BH,MAAK,GAAIA,MAAM,CAACI,SAAS,CAAC,CAAC,CAAC;QAC9B;QACA,IAAIJ,MAAM,CAACK,OAAO,CAACT,IAAI,MAAM,CAAC,EAAE;UAC9B,OAAOI,MAAM,CAACI,SAAS,CAACR,IAAI,CAACU,MAAM,EAAEN,MAAM,CAACM,MAAM,CAAC;QACrD;MACF;MACA,OAAO,EAAE;IACX;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}