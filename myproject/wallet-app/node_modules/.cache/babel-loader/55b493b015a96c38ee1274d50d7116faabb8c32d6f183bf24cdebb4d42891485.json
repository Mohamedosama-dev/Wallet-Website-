     this.recipientPhone = ''; // Reset fields after successful transfer\r\n          this.amount = 0;\r\n        })\r\n        .catch(error => {\r\n          // Handle errors (e.g., show an error message)\r\n          console.error('Error during transfer:', error);\r\n          alert('Transfer failed. Please try again.');\r\n        });\r\n    },\r\n    getCSRFToken() {\r\n      // Get CSRF token from cookies\r\n      const name = 'csrftoken=';\r\n      const decodedCookie = decodeURIComponent(document.cookie);\r\n      const cookies = decodedCookie.split(';');\r\n      for (let cookie of cookies) {\r\n        while (cookie.charAt(0) === ' ') {\r\n          cookie = cookie.substring(1);\r\n        }\r\n        if (cookie.indexOf(name) === 0) {\r\n          return cookie.substring(name.length, cookie.length);\r\n        }\r\n      }\r\n      return '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Basic Reset */\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Body Styles */\r\n.container {\r\n  font-family: Arial, sans-serif;\r\n  background-color: #f9f9f9;\r\n  color: #333;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  padding: 20px;\r\n}\r\n\r\n/* Form Container */\r\n.form-container {\r\n  background-color: #ffffff;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  padding: 20px;\r\n  max-width: 500px;\r\n  width: 100%;\r\n}\r\n\r\n/* Header Styles */\r\nh1 {\r\n  font-size: 2em;\r\n  color: #000000; /* Changed to black */\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n/* Form Styles */\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\nlabel {\r\n  margin-bottom: 5px;\r\n  font-weight: bold;\r\n}\r\n\r\ninput {\r\n  padding: 10px;\r\n  margin-bottom: 15px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  background-color: #000000; /* Changed to black */\r\n  color: #ffffff;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 1em;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #808080; /* Changed to gray on hover */\r\n}\r\n\r\n/* Back Link */\r\n.back-link {\r\n  display: block;\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  color: #000000; /* Changed to black */\r\n  text-decoration: none;\r\n}\r\n\r\n.back-link:hover {\r\n  color: #808080; /* Changed to gray on hover */\r\n  text-decoration: underline;\r\n}\r\n\r\n/* Responsive Design */\r\n@media (max-width: 600px) {\r\n  .form-container {\r\n    width: 100%;\r\n    padding: 15px;\r\n  }\r\n\r\n  h1 {\r\n    font-size: 1.5em;\r\n  }\r\n\r\n  input {\r\n    padding: 8px;\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  button {\r\n    padding: 8px;\r\n    font-size: 0.9em;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAgB;;;uBAD7BC,mBAAA,CAYM,OAZNC,UAYM,GAXJC,mBAAA,CAUM,OAVNC,UAUM,G,0BATJD,mBAAA,CAAuB,YAAnB,gBAAc,sBAClBA,mBAAA,CAMO;IANAE,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAJnBC,cAAA,KAAAC,IAAA,KAI6BC,QAAA,CAAAC,QAAA,IAAAD,QAAA,CAAAC,QAAA,IAAAF,IAAA,CAAQ;gCAC7BL,mBAAA,CAA+B,eAAxB,kBAAgB,sB,gBACvBA,mBAAA,CAAuD;IAAhDQ,IAAI,EAAC,MAAM;IAN1B,uBAAAL,MAAA,QAAAA,MAAA,MAAAM,MAAA,IAMoCC,KAAA,CAAAC,cAAc,GAAAF,MAAA;IAAEG,QAAQ,EAAR;iDAAhBF,KAAA,CAAAC,cAAc,E,6BAC1CX,mBAAA,CAAsB,eAAf,SAAO,sB,gBACdA,mBAAA,CAAiD;IAA1CQ,IAAI,EAAC,QAAQ;IAR5B,uBAAAL,MAAA,QAAAA,MAAA,MAAAM,MAAA,IAQsCC,KAAA,CAAAG,MAAM,GAAAJ,MAAA;IAAEG,QAAQ,EAAR;iDAARF,KAAA,CAAAG,MAAM,E,6BACpCb,mBAAA,CAAuC;IAA/BQ,IAAI,EAAC;EAAQ,GAAC,UAAQ,qB,4BAEhCM,YAAA,CAA8EC,sBAAA;IAAjEC,EAAE,EAAC,YAAY;IAACnB,KAAK,EAAC;;IAXzCoB,OAAA,EAAAC,QAAA,CAWqD,MAAiBf,MAAA,QAAAA,MAAA,OAXtEgB,gBAAA,CAWqD,mBAAiB,E;IAXtEC,CAAA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}